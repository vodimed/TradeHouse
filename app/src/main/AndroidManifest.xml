<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.expertek.tradehouse"
    android:versionCode="197000026"
    android:versionName="1.97.00.0026">

    <uses-permission android:name="com.honeywell.decode.permission.DECODE" /> <!--HoneyWell-->
    <uses-permission android:name="android.permission.INTERNET" /> <!--Network-->
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" /> <!--Service-->
    <!--<uses-permission android:name="android.permission.WAKE_LOCK" />--> <!--JobIntentService-->

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/barcode_example_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"> <!--android:networkSecurityConfig="@xml/network_security_config"-->

        <activity
            android:name=".exchange.TradeHouseActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/service_tradehouse"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.exchange.TradeHouseActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service
            android:name=".exchange.TradeHouseService"
            android:enabled="true"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE"
            android:process=":tradehouse" />
        <receiver
            android:name=".exchange.ServiceReceiver"
            android:enabled="true"
            android:exported="false" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"> <!-- android:configChanges="orientation|screenSize" -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>